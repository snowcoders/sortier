{
  "author": "Snowcoders",
  "bin": {
    "sortier": "./bin/index.js"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "2.22.0",
    "angular-html-parser": "1.4.0",
    "cosmiconfig": "6.0.0",
    "find-up": "4.1.0",
    "flow-parser": "0.120.1",
    "globby": "11.0.0",
    "ignore": "5.1.4",
    "postcss-less": "3.1.4",
    "postcss-scss": "2.0.0"
  },
  "description": "An opionated code sorter",
  "devDependencies": {
    "@angular/compiler": "9.0.5",
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@snowcoders/renovate-config": "1.0.6",
    "@types/chai": "4.2.10",
    "@types/estree": "0.0.42",
    "@types/globby": "8.0.0",
    "@types/mocha": "7.0.2",
    "@types/node": "13.7.7",
    "@types/sinon": "7.5.2",
    "chai": "4.2.0",
    "coveralls": "3.0.9",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "mocha": "7.1.0",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.2",
    "sinon": "9.0.0",
    "source-map-support": "0.5.16",
    "ts-node": "8.6.2",
    "tslint": "5.20.1",
    "typescript": "3.8.3"
  },
  "homepage": "https://snowcoders.github.io/sortier",
  "keywords": [
    "order",
    "alphabetize",
    "sorter",
    "sort",
    "sorting",
    "import",
    "module"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "name": "@snowcoders/sortier",
  "repository": "github:snowcoders/sortier",
  "scripts": {
    "build": "tsc",
    "build-production": "npm run build -- -p ./tsconfig.production.json",
    "clean": "rimraf .nyc_output coverage dist",
    "cli": "node bin/index.js",
    "cover": "nyc report --reporter=text-lcov | coveralls",
    "mocha": "mocha --require ts-node/register --require source-map-support/register --recursive --full-trace \"./src/**/*.test.ts\"",
    "prepare": "npm run clean && npm run build-production",
    "start": "npm run build && npm run cli",
    "test": "nyc npm run mocha",
    "test-e2e": "docker build -f e2e_tests.dockerfile ."
  },
  "version": "3.0.2"
}
