{
  "author": "Snowcoders",
  "bin": {
    "sortier": "bin/index.js"
  },
  "dependencies": {
    "@types/postcss-less": "^4.0.0",
    "@typescript-eslint/typescript-estree": "^4.0.0",
    "angular-html-parser": "^1.0.0",
    "cosmiconfig": "^7.0.0",
    "find-up": "^5.0.0",
    "flow-parser": "~0.155.0",
    "globby": "^11.0.0",
    "ignore": "^5.0.0",
    "postcss-less": "^4.0.0",
    "postcss-scss": "^4.0.0",
    "typescript": "^4.0.0"
  },
  "description": "An opionated code sorter",
  "devDependencies": {
    "@snowcoders/renovate-config": "2.0.3",
    "@types/estree": "0.0.50",
    "@types/globby": "8.0.0",
    "@types/jest": "26.0.24",
    "@types/node": "16.0.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "changelog-updater": "2.0.2",
    "coveralls": "3.1.1",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "5.1.3",
    "jest": "27.0.6",
    "lint-staged": "11.0.0",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "source-map-support": "0.5.19",
    "ts-jest": "27.0.3",
    "ts-node": "10.0.0",
    "tslint": "6.1.3"
  },
  "homepage": "https://snowcoders.github.io/sortier",
  "keywords": [
    "order",
    "alphabetize",
    "sorter",
    "sort",
    "sorting",
    "import",
    "module"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "name": "@snowcoders/sortier",
  "repository": "github:snowcoders/sortier",
  "scripts": {
    "build": "tsc -p ./tsconfig.production.json",
    "build:dev": "tsc -p ./tsconfig.json",
    "clean": "rimraf .nyc_output coverage dist",
    "cli": "node bin/index.js",
    "cover": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint --fix --debug ./src/**/*.{js,jsx,ts,tsx}",
    "mocha": "mocha --timeout 5000 --require ts-node/register --require source-map-support/register --recursive --full-trace \"./src/**/*.test.ts\"",
    "prepare": "npm run clean && npm run build",
    "start": "npm run build && npm run cli",
    "test": "jest",
    "test-e2e": "docker build -f e2e_tests.dockerfile .",
    "version": "changelog-updater && git add ."
  },
  "version": "3.3.0"
}
