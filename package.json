    "@types/chai": "4.1.7",
    "@types/estree": "0.0.39",
    "@types/globby": "8.0.0",
    "@types/mocha": "5.2.6",
    "@types/node": "12.0.0",
    "@types/sinon": "7.0.11",
    "chai": "4.2.0",
    "coveralls": "3.0.3",
    "husky": "2.2.0",
    "lint-staged": "8.1.6",
    "mocha": "6.1.4",
    "nyc": "14.1.0",
    "prettier": "1.17.0",
    "sinon": "7.3.2",
