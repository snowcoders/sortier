{
  "name": "@snowcoders/sortier",
  "version": "2.1.8",
  "description": "An opionated code sorter",
  "bin": {
    "sortier": "./bin/index.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "build-production": "npm run build -- -p ./tsconfig.production.json",
    "clean": "rimraf .nyc_output coverage dist",
    "cover": "nyc npm run mocha",
    "precommit": "lint-staged",
    "prettier": "prettier",
    "prepare": "npm run clean && npm run build-production",
    "start": "npm run build && node bin/index.js",
    "test": "npm run mocha",
    "mocha": "mocha --require ts-node/register --require source-map-support/register --full-trace src/**/*.test.ts"
  },
  "lint-staged": {
    "**/*.ts": [
      "prettier --write",
      "node bin/index.js",
      "git add"
    ]
  },
  "keywords": [
    "sort",
    "sorting",
    "import",
    "module"
  ],
  "author": "Snowcoders",
  "license": "MIT",
  "repository": "github:snowcoders/sortier",
  "devDependencies": {
    "@types/chai": "4.1.4",
    "@types/estree": "0.0.39",
    "@types/globby": "8.0.0",
    "@types/mocha": "5.2.5",
    "@types/node": "10.5.7",
    "chai": "4.1.2",
    "husky": "0.14.3",
    "jsdom": "11.12.0",
    "karma": "2.0.5",
    "karma-chai": "0.1.0",
    "karma-coverage": "1.1.2",
    "karma-jsdom-launcher": "6.1.3",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-source-map-support": "1.3.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-sourcemap-writer": "0.1.2",
    "karma-typescript": "3.0.13",
    "karma-typescript-es6-transform": "1.0.4",
    "karma-typescript-preprocessor": "0.3.1",
    "lint-staged": "7.2.0",
    "mocha": "5.2.0",
    "mocha-typescript": "1.1.17",
    "nyc": "12.0.2",
    "prettier": "1.14.2",
    "source-map-support": "0.5.8",
    "ts-node": "7.0.0"
  },
  "dependencies": {
    "cosmiconfig": "^5.0.5",
    "flow-parser": "^0.78.0",
    "fs": "0.0.2",
    "globby": "^8.0.1",
    "rimraf": "^2.6.2",
    "typescript": "^3.0.0",
    "typescript-eslint-parser": "^18.0.0"
  }
}