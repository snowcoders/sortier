{
  "name": "@snowcoders/sortier",
  "version": "2.1.14",
  "description": "An opionated code sorter",
  "bin": {
    "sortier": "./bin/index.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "build-production": "npm run build -- -p ./tsconfig.production.json",
    "clean": "rimraf .nyc_output coverage dist",
    "cover": "nyc npm run mocha",
    "prepare": "npm run clean && npm run build-production",
    "start": "npm run build && node bin/index.js",
    "test": "npm run mocha",
    "mocha": "mocha --require ts-node/register --require source-map-support/register --full-trace src/**/*.test.ts"
  },
  "keywords": [
    "sort",
    "sorting",
    "import",
    "module"
  ],
  "author": "Snowcoders",
  "license": "MIT",
  "repository": "github:snowcoders/sortier",
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/estree": "0.0.39",
    "@types/globby": "8.0.0",
    "@types/mocha": "5.2.5",
    "@types/node": "10.12.1",
    "chai": "4.2.0",
    "husky": "1.1.2",
    "lint-staged": "7.3.0",
    "mocha": "5.2.0",
    "nyc": "13.1.0",
    "prettier": "1.14.3",
    "source-map-support": "0.5.9",
    "ts-node": "7.0.1"
  },
  "dependencies": {
    "cosmiconfig": "^5.0.6",
    "flow-parser": "^0.84.0",
    "fs": "0.0.2",
    "globby": "^8.0.1",
    "rimraf": "^2.6.2",
    "typescript": "^3.0.3",
    "typescript-estree": "^5.0.0"
  }
}
