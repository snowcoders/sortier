{
  "author": "Snowcoders",
  "bin": {
    "sortier": "./bin/index.js"
  },
  "dependencies": {
    "@types/postcss-less": "^4.0.0",
    "@typescript-eslint/typescript-estree": "^4.0.0",
    "angular-html-parser": "^1.0.0",
    "cosmiconfig": "^7.0.0",
    "find-up": "^5.0.0",
    "flow-parser": "~0.158.0",
    "globby": "^11.0.0",
    "ignore": "^5.0.0",
    "postcss-less": "^5.0.0",
    "postcss-scss": "^4.0.0",
    "typescript": "^4.0.0"
  },
  "description": "An opinionated code sorter",
  "devDependencies": {
    "@snowcoders/renovate-config": "3.0.0-beta.2",
    "@types/estree": "0.0.50",
    "@types/jest": "27.0.2",
    "@types/node": "16.10.1",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "changelog-updater": "2.0.2",
    "coveralls": "3.1.1",
    "docsify-cli": "4.4.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "5.1.3",
    "jest": "27.2.2",
    "lint-staged": "11.1.2",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "source-map-support": "0.5.20",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "tslint": "6.1.3"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "exports": "./dist/lib/index.js",
  "homepage": "https://snowcoders.github.io/sortier",
  "keywords": [
    "order",
    "alphabetize",
    "sorter",
    "sort",
    "sorting",
    "organize",
    "import",
    "module"
  ],
  "license": "MIT",
  "name": "sortier",
  "repository": "github:snowcoders/sortier",
  "scripts": {
    "build": "tsc -p ./tsconfig.production.json",
    "build:dev": "tsc -p ./tsconfig.json",
    "clean": "rimraf coverage dist",
    "cover": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint --fix ./src/**/*.{js,jsx,ts,tsx}",
    "mocha": "mocha --timeout 5000 --require ts-node/register --require source-map-support/register --recursive --full-trace \"./src/**/*.test.ts\"",
    "prepare": "npm run clean && npm run build",
    "start": "node bin/index.js",
    "start-docs": "docsify serve ./docs",
    "test": "jest",
    "test-e2e": "docker build -f e2e_tests.dockerfile .",
    "version": "changelog-updater && git add ."
  },
  "sideEffects": false,
  "type": "module",
  "version": "0.0.0"
}
