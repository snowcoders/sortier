{
  "author": "Snowcoders",
  "bin": {
    "sortier": "./bin/index.js"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "2.0.0",
    "angular-html-parser": "1.2.0",
    "cosmiconfig": "5.2.1",
    "find-up": "4.1.0",
    "flow-parser": "0.105.2",
    "globby": "10.0.0",
    "ignore": "5.1.2",
    "postcss-less": "3.1.4",
    "postcss-scss": "2.0.0"
  },
  "description": "An opionated code sorter",
  "devDependencies": {
    "@angular/compiler": "8.2.2",
    "@snowcoders/renovate-config": "1.0.4",
    "@types/chai": "4.2.0",
    "@types/estree": "0.0.39",
    "@types/globby": "8.0.0",
    "@types/mocha": "5.2.7",
    "@types/node": "12.7.1",
    "@types/sinon": "7.0.13",
    "chai": "4.2.0",
    "coveralls": "3.0.6",
    "husky": "3.0.3",
    "lint-staged": "9.2.1",
    "mocha": "6.2.0",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "rimraf": "2.7.0",
    "sinon": "7.4.1",
    "source-map-support": "0.5.13",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.5.2"
  },
  "homepage": "https://snowcoders.github.io/sortier",
  "keywords": [
    "order",
    "alphabetize",
    "sorter",
    "sort",
    "sorting",
    "import",
    "module"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "name": "@snowcoders/sortier",
  "repository": "github:snowcoders/sortier",
  "scripts": {
    "build": "tsc",
    "build-production": "npm run build -- -p ./tsconfig.production.json",
    "clean": "rimraf .nyc_output coverage dist",
    "cover": "nyc report --reporter=text-lcov | coveralls",
    "mocha": "mocha --require ts-node/register --require source-map-support/register --full-trace \"./src/**/*.test.ts\"",
    "prepare": "npm run clean && npm run build-production",
    "run": "node bin/index.js",
    "start": "npm run build && npm run run",
    "test": "nyc npm run mocha",
    "test-e2e": "docker build -f e2e_tests.dockerfile ."
  },
  "version": "2.6.2"
}
