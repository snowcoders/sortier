{
  "author": "Snowcoders",
  "bin": {
    "sortier": "./bin/index.js"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "4.0.1",
    "angular-html-parser": "1.7.1",
    "cosmiconfig": "7.0.0",
    "find-up": "5.0.0",
    "flow-parser": "0.133.0",
    "globby": "11.0.1",
    "ignore": "5.1.8",
    "postcss-less": "3.1.4",
    "postcss-scss": "2.1.1",
    "typescript": "4.0.2"
  },
  "description": "An opionated code sorter",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@snowcoders/renovate-config": "2.0.1",
    "@types/chai": "4.2.12",
    "@types/estree": "0.0.45",
    "@types/globby": "8.0.0",
    "@types/mocha": "8.0.3",
    "@types/node": "14.6.4",
    "@types/sinon": "9.0.5",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "chai": "4.2.0",
    "coveralls": "3.1.0",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "lint-staged": "10.2.11",
    "mocha": "8.1.1",
    "nyc": "15.1.0",
    "prettier": "2.1.1",
    "rimraf": "3.0.2",
    "sinon": "9.0.3",
    "source-map-support": "0.5.19",
    "ts-node": "9.0.0",
    "tslint": "6.1.3"
  },
  "homepage": "https://snowcoders.github.io/sortier",
  "keywords": [
    "order",
    "alphabetize",
    "sorter",
    "sort",
    "sorting",
    "import",
    "module"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "name": "@snowcoders/sortier",
  "repository": "github:snowcoders/sortier",
  "scripts": {
    "build": "tsc",
    "build-production": "npm run build -- -p ./tsconfig.production.json",
    "clean": "rimraf .nyc_output coverage dist",
    "cli": "node bin/index.js",
    "cover": "nyc report --reporter=text-lcov | coveralls",
    "mocha": "mocha --timeout 5000 --require ts-node/register --require source-map-support/register --recursive --full-trace \"./src/**/*.test.ts\"",
    "prepare": "npm run clean && npm run build-production",
    "start": "npm run build && npm run cli",
    "test": "nyc npm run mocha",
    "test-e2e": "docker build -f e2e_tests.dockerfile ."
  },
  "version": "3.1.3"
}
