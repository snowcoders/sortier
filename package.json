{
  "name": "@snowcoders/sortier",
  "version": "2.1.7",
  "description": "An opionated code sorter",
  "bin": {
    "sortier": "./bin/index.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf .nyc_output coverage dist",
    "cover": "nyc npm run mocha",
    "precommit": "lint-staged",
    "prepare": "npm run clean && npm run build",
    "start": "npm run build && node bin/index.js",
    "test": "npm run mocha",
    "mocha": "mocha --require ts-node/register --require source-map-support/register --full-trace src/**/*.test.ts"
  },
  "lint-staged": {
    "**/*.ts": [
      "node bin/index.js",
      "git add"
    ]
  },
  "keywords": [
    "sort",
    "sorting",
    "import",
    "module"
  ],
  "author": "Snowcoders",
  "license": "MIT",
  "repository": "github:snowcoders/sortier",
  "devDependencies": {
    "@types/chai": "4.1.4",
    "@types/estree": "0.0.39",
    "@types/globby": "6.1.0",
    "@types/mocha": "2.2.48",
    "@types/node": "10.5.2",
    "chai": "4.1.2",
    "husky": "0.14.3",
    "jsdom": "11.11.0",
    "karma": "2.0.4",
    "karma-chai": "0.1.0",
    "karma-coverage": "1.1.2",
    "karma-jsdom-launcher": "6.1.3",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-source-map-support": "1.3.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-sourcemap-writer": "0.1.2",
    "karma-typescript": "3.0.13",
    "karma-typescript-es6-transform": "1.0.4",
    "karma-typescript-preprocessor": "0.3.1",
    "lint-staged": "7.2.0",
    "mocha": "4.1.0",
    "mocha-typescript": "1.1.16",
    "nyc": "11.9.0",
    "source-map-support": "0.5.6",
    "ts-node": "7.0.0"
  },
  "dependencies": {
    "cosmiconfig": "^5.0.5",
    "flow-parser": "^0.76.0",
    "fs": "0.0.1-security",
    "globby": "^8.0.1",
    "rimraf": "^2.6.2",
    "typescript": "^2.9.2",
    "typescript-eslint-parser": "^15.0.1"
  }
}
