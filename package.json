{
  "author": "Snowcoders",
  "bin": {
    "sortier": "./bin/index.js"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "2.7.0",
    "angular-html-parser": "1.3.0",
    "cosmiconfig": "6.0.0",
    "find-up": "4.1.0",
    "flow-parser": "0.112.0",
    "globby": "10.0.0",
    "ignore": "5.1.4",
    "postcss-less": "3.1.4",
    "postcss-scss": "2.0.0"
  },
  "description": "An opionated code sorter",
  "devDependencies": {
    "@angular/compiler": "8.2.13",
    "@istanbuljs/nyc-config-typescript": "0.1.3",
    "@snowcoders/renovate-config": "1.0.4",
    "@types/chai": "4.2.4",
    "@types/estree": "0.0.39",
    "@types/globby": "8.0.0",
    "@types/mocha": "5.2.7",
    "@types/node": "12.12.7",
    "@types/sinon": "7.5.0",
    "chai": "4.2.0",
    "coveralls": "3.0.7",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "mocha": "6.2.2",
    "nyc": "14.1.1",
    "prettier": "1.19.1",
    "rimraf": "3.0.0",
    "sinon": "7.5.0",
    "source-map-support": "0.5.16",
    "ts-node": "8.5.0",
    "tslint": "5.20.1",
    "typescript": "3.7.2"
  },
  "homepage": "https://snowcoders.github.io/sortier",
  "keywords": [
    "order",
    "alphabetize",
    "sorter",
    "sort",
    "sorting",
    "import",
    "module"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "name": "@snowcoders/sortier",
  "repository": "github:snowcoders/sortier",
  "scripts": {
    "build": "tsc",
    "build-production": "npm run build -- -p ./tsconfig.production.json",
    "clean": "rimraf .nyc_output coverage dist",
    "cli": "node bin/index.js",
    "cover": "nyc report --reporter=text-lcov | coveralls",
    "mocha": "mocha --require ts-node/register --require source-map-support/register --recursive --full-trace \"./src/**/*.test.ts\"",
    "prepare": "npm run clean && npm run build-production",
    "start": "npm run build && npm run cli",
    "test": "nyc npm run mocha",
    "test-e2e": "docker build -f e2e_tests.dockerfile ."
  },
  "version": "3.0.1"
}
